{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "answers": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "holeId": {
            "type": "string"
          },
          "answerText": {
            "type": "string"
          }
        }
      }
    },
    "required": ["holeId","answerText"]
  }
}
