{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "meta": {
      "$ref": "http://json-quiz.github.io/json-quiz/schemas/metadata/schema.json"
    },
    "steps": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": { "$ref": "http://json-quiz.github.io/json-quiz/schemas/step/schema.json" }
    }
  },
  "required": ["id", "steps"]
}
